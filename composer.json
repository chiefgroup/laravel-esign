{
    "name": "xnxktech/laravel-esign",
    "description": "Provide eSign (electronic contract signing) API for Laravel",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "doctrine/cache": "^1.10",
        "monolog/monolog": "^2.0",
        "pimple/pimple": "^3.0",
        "symfony/http-foundation": "^4.0 || ^5.0",
        "symfony/psr-http-message-bridge": "^1.0 || ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "illuminate/support": "^9.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/phpinsights": "^2.1",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "YGNICE",
            "email": "ygnice888@gmail.com"
        }
    ],
 	"autoload": {
        "psr-4": {
            "XNXK\\LaravelEsign\\": "src/"
        }
    },
    "config": {
    	"sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XNXK\\LaravelEsign\\ServiceProvider"
            ]
        }
    }
}
