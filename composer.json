{
    "name": "chiefgroup/laravel-esign",
    "description": "QF Laravel ESign API v2",
    "type": "library",
    "authors": [
        {
            "name": "peng",
            "email": "2512422541@qq.com"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.3",
        "monolog/monolog": "^1.22 || ^2.0",
        "overtrue/http": "^1.2",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^3.3 || ^4.3 || ^5.0 || ^6.0",
        "symfony/http-foundation": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "laravel/framework": "^8",
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "^9.3"
    },
 	"autoload": {
        "psr-4": {
            "QF\\LaravelEsign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QF\\LaravelEsign\\Tests\\": "tests/"
        }
    },
    "config": {
    	"sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "QF\\LaravelEsign\\ServiceProvider"
            ],
            "aliases": {
                "Esign": "QF\\LaravelEsign\\EsignFacade"
            }
        }
    },
    "scripts": {
        "test": "phpunit --colors",
        "fix-style": "php-cs-fixer fix --using-cache=no --ansi"
    },
    "license": "MIT"
}
